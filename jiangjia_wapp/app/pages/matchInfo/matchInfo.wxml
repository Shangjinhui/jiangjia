                class="{{item.home_id==analyInfo.away_team_id&&item.home_scores>item.away_scores?'coBC0100':''}}">{{item.home_scores}}</text>-<text
                class="{{item.away_id==analyInfo.away_team_id&&item.home_scores<item.away_scores?'coBC0100':''}}">{{item.away_scores}}</text>
              <view class="fsize20 coA0A0A0">（{{item.home_half_scores}}-{{item.away_half_scores}}）</view>
            </view>
            <view
              class="fb fb-col fb-main-center  wid138 left {{item.away_id==analyInfo.away_team_id&&item.home_scores<item.away_scores?'coBC0100':''}}">
              {{item.away_team_name}}</view>
            <view
              class="{{item.odds_status==-1?'co4FC419':''}} {{item.odds_status==0?'co3F88C5':''}} {{item.odds_status==1?'coBC0100':''}} center fb fb-col fb-main-center  wid86 ">
              {{item.odds}}
