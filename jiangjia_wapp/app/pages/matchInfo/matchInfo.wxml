<!--pages/matchInfo/matchInfo.wxml-->
<view class="box pb30 ovh">
  <view class="cofff match-top center box pt20"
    style="background:url({{imgUrl+'match_bg11.png'}}) no-repeat center bottom/750rpx 451rpx;">
    <view class="fsize36 lh50">{{matchInfo.competition_name}}</view>
    <view class="fsize22 opa5 mb50">{{matchInfo.matchTime}}</view>
    <view class="">
      <navigator hover-class="none" open-type="{{isHome?'reLaunch':'navigate'}}"
        url="/pages/teamInfo/teamInfo?id={{matchInfo.home_team_id}}" class="rel match-top-con inline fsize28">
        <image src="{{matchInfo.home_team_logo?matchInfo.home_team_logo:imgUrl+'banner_1.png'}}" mode='aspectFit'
          class="radius-50 block"></image>
        <view class="abs">{{matchInfo.home_team_name}}</view>
      </navigator>
      <view class="rel inline match-top-num fsize24">
        <view class="fsize66 bold"
          wx:if="{{matchInfo.status_id==1||matchInfo.status_id==9||matchInfo.status_id==12||matchInfo.status_id==13}}">
          VS</view>
        <view class="fsize66 bold" wx:else>{{matchInfo.home_scores[0]}}-{{matchInfo.away_scores[0]}}</view>
        <view class="abs tx ">
          <!-- <view wx:if="{{matchInfo.status_id==2||matchInfo.status_id==4||matchInfo.status_id==5}}">{{matchInfo.time>90?'90+':matchInfo.time}}”</view> -->
          <view class="fl w33 center " wx:if="{{matchInfo.status_id==2}}">{{matchInfo.time>45?'45+':matchInfo.time}}'
          </view>
          <view class="fl w33 center " wx:if="{{matchInfo.status_id==4||matchInfo.status_id==5}}">
            {{matchInfo.time>90?'90+':matchInfo.time}}'</view>
          <view wx:if="{{matchInfo.status_id==1}}">未</view>
          <view wx:if="{{matchInfo.status_id==3}}">中</view>
          <view wx:if="{{matchInfo.status_id==7}}">点球</view>
          <view wx:if="{{matchInfo.status_id==8}}">完</view>
          <view wx:if="{{matchInfo.status_id==9}}">推迟</view>
          <view wx:if="{{matchInfo.status_id==10}}">中断</view>
          <view wx:if="{{matchInfo.status_id==11}}">腰斩</view>
          <view wx:if="{{matchInfo.status_id==12}}">取消</view>
          <view wx:if="{{matchInfo.status_id==13}}">待定</view>
        </view>
        <view class="abs tx tx1"
          wx:if="{{matchInfo.status_id!=1&&matchInfo.status_id!=9&&matchInfo.status_id!=12&&matchInfo.status_id!=13&&matchInfo.status_id!=2}}">
          （半:{{matchInfo.home_scores[1]}}-{{matchInfo.away_scores[1]}}）</view>
      </view>
      <navigator hover-class="none" open-type="{{isHome?'reLaunch':'navigate'}}"
        url="/pages/teamInfo/teamInfo?id={{matchInfo.away_team_id}}" class="rel match-top-con inline fsize28">
        <image src="{{matchInfo.away_team_logo?matchInfo.away_team_logo:imgUrl+'banner_1.png'}}" mode='aspectFit'
          class="radius-50 block"></image>
        <view class="abs">{{matchInfo.away_team_name}}</view>
      </navigator>
    </view>
  </view>


  <view class="fb bgfff center fsize24 bold lh72 rel home-top">
    <view class="fb fb-col item-grow-1 fb-cross-center  {{nav==item.id?'coE71415':''}}" bindtap="changeNav"
      data-nav="{{item.id}}" wx:for="{{navList}}" wx:key="index">{{item.name}}</view>
  </view>

  <view class="fb bgfff center fsize24 bold lh72 rel home-top on" wx:if="{{isFixed}}">
    <view class="fb fb-col item-grow-1 fb-cross-center  {{nav==item.id?'coE71415':''}}" bindtap="changeNav"
      data-nav="{{item.id}}" wx:for="{{navList}}" wx:key="index">{{item.name}}</view>
  </view>


  <!-- 赛况 -->
  <view class="pd24 pt25 race" hidden="{{nav!=1}}">
    <view class="f-cb fsize24 lh22 mb20">
      <view class="fl pl25" style="border-left:4rpx solid #E71415;">{{matchInfo.home_team_name}}</view>
      <view class="fr pr25" style="border-right:4rpx solid #1C80E7;">{{matchInfo.away_team_name}}</view>
    </view>
    <view class="bgfff radius10 pt30 race-num mb40">
      <view class="f-cb center fsize24  bor-boD8D8D8">
        <view class="fl w33 bor-riD8D8D8 box">
          <view class="mb20">进攻</view>
          <view class="rel inline fsize22 hei70 wid70">
            <canvas class='oneCan' canvas-id='one' hidden="{{isFixed}}"></canvas>
            <view class="abs tx">{{matchInfo.home_attack}}</view>
            <view class="abs tx tx1">{{matchInfo.away_attack}}</view>
          </view>
        </view>
        <view class="fl w33 bor-riD8D8D8 box">
          <view class="mb20">危险进攻</view>
          <view class="rel inline fsize22 hei70 wid70">
            <canvas class='oneCan' canvas-id='two' hidden="{{isFixed}}"></canvas>
            <view class="abs tx">{{matchInfo.home_dangerous_attack}}</view>
            <view class="abs tx tx1">{{matchInfo.away_dangerous_attack}}</view>
          </view>
        </view>
        <view class="fl w33 ">
          <view class="mb20">控球率</view>
          <view class="rel inline fsize22 hei70 wid70">
            <canvas class='oneCan' canvas-id='three' hidden="{{isFixed}}"></canvas>
            <view class="abs tx">{{matchInfo.home_ball_control}}</view>
            <view class="abs tx tx1">{{matchInfo.away_ball_control}}</view>
          </view>
        </view>
      </view>
      <view class="f-cb pt20 pb20 ">
        <view class="fl fb fsize22 race-num-fl pt20">
          <view class="fb fb-col item-grow-1 fb-cross-center">
            <image src="{{imgUrl+'match_ico9.png'}}" class="img1"></image>
            <view>{{matchInfo.home_scores[4]}}</view>
          </view>
          <view class="fb fb-col item-grow-1 fb-cross-center">
            <image src="{{imgUrl+'match_ico13.png'}}" class="img1"></image>
            <view>{{matchInfo.home_scores[2]}}</view>
          </view>
          <view class="fb fb-col item-grow-1 fb-cross-center">
            <image src="{{imgUrl+'match_ico12.png'}}" class="img1"></image>
            <view>{{matchInfo.home_scores[3]}}</view>
          </view>
        </view>

        <view class="fl race-num-cen center fsize20">
          <view class="mb5">射正球门</view>
          <view class="line rel marginauto mb10">
            <view class="on" style="width:{{222*matchInfo.onScale}}rpx;"></view>
            <view class="abs tx">{{matchInfo.home_shots_on_target}}</view>
            <view class="abs tx tx1">{{matchInfo.away_shots_on_target}}</view>
          </view>
          <view class="mb5 ">射偏球门</view>
          <view class="line rel marginauto">
            <view class="on" style="width:{{222*matchInfo.outScale}}rpx;"></view>
            <view class="abs tx">{{matchInfo.home_shots_out_target}}</view>
            <view class="abs tx tx1">{{matchInfo.away_shots_out_target}}</view>
          </view>
        </view>


        <view class="fr fb fsize22 race-num-fl pt20">
          <view class="fb fb-col item-grow-1 fb-cross-center">
            <image src="{{imgUrl+'match_ico12.png'}}" class="img1"></image>
            <view>{{matchInfo.away_scores[3]}}</view>
          </view>
          <view class="fb fb-col item-grow-1 fb-cross-center">
            <image src="{{imgUrl+'match_ico13.png'}}" class="img1"></image>
            <view>{{matchInfo.away_scores[2]}}</view>
          </view>
          <view class="fb fb-col item-grow-1 fb-cross-center">
            <image src="{{imgUrl+'match_ico9.png'}}" class="img1"></image>
            <view>{{matchInfo.away_scores[4]}}</view>
          </view>
        </view>

      </view>

    </view>

    <view class="f-cb  home-expert-top center fsize26 bold " wx:if="{{tliveList.length>0}}">
      <view class="fl {{state==1?'on':''}}" bindtap="changeState" data-cur="1">文字直播</view>
      <view class="fl {{state==2?'on':''}}" bindtap="changeState" data-cur="2">重要事件</view>
    </view>


    <view class="race-tx fsize26 " wx:if="{{state==1}}">
      <view class="rel race-tx-con mb20 {{item.position==1?'red':''}}  {{item.position==2?'blue':''}}"
        wx:for="{{tliveList}}" wx:key="index">

        <!-- 1进球，2角球，3黄牌，4红牌，5界外球，6任意球，7球门球，8点球，9换人，10比赛开始，11中场，12结束，13半场比分，15两黄变红，16点球未进，17乌龙球，19伤停补时，21射正，22射偏，23进攻，24危险进攻，25控球率，26加时赛结束,27点球大战结束 -->
        <image src="{{imgUrl+'match_ico10.png'}}" class="abs" wx:if="{{item.type==1}}"></image>
        <image src="{{imgUrl+'match_ico9.png'}}" class="abs" wx:elif="{{item.type==2}}"></image>
        <image src="{{imgUrl+'match_ico12.png'}}" class="abs" wx:elif="{{item.type==3}}"></image>
        <image src="{{imgUrl+'match_ico13.png'}}" class="abs" wx:elif="{{item.type==4}}"></image>
        <image src="{{imgUrl+'match_ico16.png'}}" class="abs" wx:elif="{{item.type==8}}"></image>
        <image src="{{imgUrl+'match_ico11.png'}}" class="abs" wx:elif="{{item.type==9}}"></image>
        <image src="{{imgUrl+'match_ico29.png'}}" class="abs" wx:elif="{{item.type==15}}"></image>
        <image src="{{imgUrl+'match_ico17.png'}}" class="abs" wx:elif="{{item.type==16}}"></image>
        <image src="{{imgUrl+'match_ico18.png'}}" class="abs" wx:elif="{{item.type==17}}"></image>
        <image src="{{imgUrl+'match_ico14.png'}}" class="abs" wx:else="{{item.type==0}}"></image>
        <!-- <image src="{{imgUrl+'match_ico19.png'}}" class="abs" wx:else="{{item.type==23}}"></image> -->

        {{item.data}}
      </view>

    </view>


    <view wx:if="{{state==2}}">
      <view class="center race-text">
        <image src="{{imgUrl+'match_ico14.png'}}" class="img1 mb10"></image>
        <view class="line rel pt20">
          <view class="line-con  {{index<incidentsList.length-1?'mb30':''}}" wx:for="{{incidentsList}}" wx:key="index">
            <view class="f-cb " wx:if="{{item.position!=0}}">
              <view class="{{item.position==1?'fl right':'fr left'}} fsize22">
                <view class="race-text-con rel">
                  <!-- 1进球，2角球，3黄牌，4红牌，5界外球，6任意球，7球门球，8点球，9换人，10比赛开始，11中场，12结束，13半场比分，15两黄变红，16点球未进，17乌龙球，19伤停补时，21射正，22射偏，23进攻，24危险进攻，25控球率，26加时赛结束,27点球大战结束 -->
                  <view wx:if="{{item.type==1||item.type==8||item.type==17}}">{{item.home_score}}-{{item.away_score}}
                  </view>
                  <view class="{{item.position==1?'pr40':'pl40'}} lh36 rel text">
                    <image src="{{imgUrl+'match_ico10.png'}}" class="abs" wx:if="{{item.type==1}}"></image>
                    <image src="{{imgUrl+'match_ico9.png'}}" class="abs" wx:elif="{{item.type==2}}"></image>
                    <image src="{{imgUrl+'match_ico12.png'}}" class="abs" wx:elif="{{item.type==3}}"></image>
                    <image src="{{imgUrl+'match_ico13.png'}}" class="abs" wx:elif="{{item.type==4}}"></image>
                    <image src="{{imgUrl+'match_ico16.png'}}" class="abs" wx:elif="{{item.type==8}}"></image>



                    <!-- <image src="{{imgUrl+'match_ico34.png'}}" class="abs" wx:elif="{{item.type==9}}"></image>
                    <image src="{{imgUrl+'match_ico35.png'}}" class="abs" wx:elif="{{item.type==9}}"></image> -->


                    <image src="{{imgUrl+'match_ico29.png'}}" class="abs" wx:elif="{{item.type==15}}"></image>
                    <image src="{{imgUrl+'match_ico17.png'}}" class="abs" wx:elif="{{item.type==16}}"></image>
                    <image src="{{imgUrl+'match_ico18.png'}}" class="abs" wx:elif="{{item.type==17}}"></image>
                    <!-- <image src="{{imgUrl+'match_ico19.png'}}" class="abs" wx:else="{{item.type==23}}"></image> -->

                    <view wx:if="{{item.player_name}}">{{item.player_name}}</view>
                    <view wx:if="{{item.in_player_name}}" class="rel">{{item.in_player_name}}
                      <image src="{{imgUrl+'match_ico34.png'}}" class="abs img1"></image>
                    </view>
                    <view wx:if="{{item.out_player_name}}" class="rel">{{item.out_player_name}}
                      <image src="{{imgUrl+'match_ico35.png'}}" class="abs img1"></image>
                    </view>
                  </view>
                  <view class="num">{{item.time}}</view>
                </view>
              </view>
            </view>
            <view class="line-con-tx fsize22" wx:if="{{(item.type==11||item.type==12)}}">
              {{item.type==11?'中场':'结束'}} {{item.home_score}}-{{item.away_score}}
            </view>
            <view class="line-con-tx fsize22" wx:if="{{(item.type==19)}}">
              伤停补时
            </view>
          </view>
        </view>
        <image src="{{imgUrl+'match_ico20.png'}}" class="img1 mt10"></image>
      </view>
    </view>
    <!-- 天气 -->
    <view class="race-weat box f-cb fsize24 cofff mb20 rel"
      style="background:url({{imgUrl+'match_bg2.png'}}) no-repeat center bottom/698rpx 145rpx;"
      wx:if="{{matchInfo.humidity||matchInfo.temperature||matchInfo.wind||matchInfo.pressure}}">
      <view class="fl">
        <image src="{{imgUrl+'match_ico21.png'}}" class="fl mr10"></image> 湿度
        :{{matchInfo.humidity?matchInfo.humidity:'暂无'}}
      </view>
      <view class="fl">
        <image src="{{imgUrl+'match_ico22.png'}}" class="fl mr10"></image> 温度
        :{{matchInfo.temperature?matchInfo.temperature:'暂无'}}
      </view>
      <view class="fl">
        <image src="{{imgUrl+'match_ico23.png'}}" class="fl mr10"></image> 风速 :{{matchInfo.wind?matchInfo.wind:'暂无'}}
      </view>
      <view class="fl">
        <image src="{{imgUrl+'match_ico24.png'}}" class="fl mr10"></image> 气压
        :{{matchInfo.pressure?matchInfo.pressure:'暂无'}}
      </view>

      <view class="abs tx">天气：
        <text wx:if="{{matchInfo.weather==1}}">局部有云</text>
        <text wx:if="{{matchInfo.weather==2}}">多云</text>
        <text wx:if="{{matchInfo.weather==3}}">局部有云/雨</text>
        <text wx:if="{{matchInfo.weather==4}}">雪</text>
        <text wx:if="{{matchInfo.weather==5}}">晴</text>
        <text wx:if="{{matchInfo.weather==6}}">阴有雨/局部有雷暴</text>
        <text wx:if="{{matchInfo.weather==7}}">阴</text>
        <text wx:if="{{matchInfo.weather==8}}">薄雾</text>
        <text wx:if="{{matchInfo.weather==9}}">阴有雨</text>
        <text wx:if="{{matchInfo.weather==10}}">多云有雨</text>
        <text wx:if="{{matchInfo.weather==11}}">多云有雨/局部有雷暴</text>
        <text wx:if="{{matchInfo.weather==12}}">局部有云/雨和雷暴</text>
        <text wx:if="{{matchInfo.weather==13}}">雾</text>
      </view>
    </view>

    <image src="{{imgUrl+'match_ico15.png'}}" class="fix-bot" bindtap="showTip"></image>


    <view class="race-brand box  fsize22 co666 f-cb lh36 trf {{tipShow?'on':''}} {{isIphoneX?'race-brand1':''}}"
      bindtap="showTip" style="background:#fff url({{imgUrl+'match_ico25.png'}}) no-repeat top left/24rpx 112rpx;">
      <view class="fl">
        <image src="{{imgUrl+'match_ico10.png'}}" class="fl mr5"></image>进球
      </view>
      <view class="fl">
        <image src="{{imgUrl+'match_ico16.png'}}" class="fl mr5"></image>点球
      </view>
      <view class="fl">
        <image src="{{imgUrl+'match_ico17.png'}}" class="fl mr5"></image>点球未进
      </view>
      <view class="fl">
        <image src="{{imgUrl+'match_ico18.png'}}" class="fl mr5"></image>乌龙球
      </view>
      <view class="fl">
        <image src="{{imgUrl+'match_ico19.png'}}" class="fl mr5"></image>助攻
      </view>
      <view class="fl">
        <image src="{{imgUrl+'match_ico9.png'}}" class="fl mr5"></image>角球
      </view>
      <view class="fl">
        <image src="{{imgUrl+'match_ico12.png'}}" class="fl mr5"></image>黄牌
      </view>
      <view class="fl">
        <image src="{{imgUrl+'match_ico13.png'}}" class="fl mr5"></image>红牌
      </view>
      <view class="fl">
        <image src="{{imgUrl+'match_ico11.png'}}" class="fl mr5"></image>换人
      </view>
      <view class="fl">
        <image src="{{imgUrl+'match_ico29.png'}}" class="fl mr5"></image>两黄一红
      </view>
    </view>

  </view>

  <!-- 直播 -->
  <view class="pd24 pt25" wx:if="{{nav==2}}">
    <navigator hover-class="none" open-type="{{isHome?'reLaunch':'navigate'}}"
      url="/pages/liveInfo/liveInfo?id={{item.id}}" class="mb20 bgfff radius6 home-match fsize20" wx:for="{{list}}"
      wx:key="index">
      <view class="f-cb mb10 coA0A0A0">
        <view class="fl w33">
          <view class="fl mr20">周{{item.ftime}}</view>
          <view class="co333 fl">{{item.competition}}</view>
        </view>
        <view class="fl w33 center">{{item.time}}</view>
        <view class="fr coE71415" wx:if="{{item.status==1}}">直播中</view>
        <view class="fr coFEA830" wx:elif="{{item.status==0&&item.price>0}}">预售中</view>
        <view class="fr coA0A0A0" wx:else>未开始</view>
      </view>

      <view class=" lh70 f-cb co333  fsize28 mb15 home-match-con">
        <view class="fl wid266 right">{{item.home}}
          <image src="{{item.home_logo?item.home_logo:imgUrl+'banner_1.png'}}" class="ml20" mode="aspectFit"></image>
        </view>
        <view class="fl wid110 coccc center">VS</view>
        <view class="fl wid266">
          <image src="{{item.away_logo?item.away_logo:imgUrl+'banner_1.png'}}" class="mr20" mode="aspectFit"></image>
          {{item.away}}
        </view>
      </view>

      <view class=" f-cb coA0A0A0 lh34">
        <view class="rel fl mr20">
          <image src="{{item.expert.avatar?item.expert.avatar:imgUrl+'tx.png'}}" mode='aspectFill'
            class="block radius-50  wid34 hei34"></image>
          <image src="{{imgUrl+'home_ico4.png'}}" mode='aspectFill' class="abs bottom-0 right-0 wid10 hei10"></image>
        </view>
        <view class="fl mr10">{{item.expert.nickName}}</view>
        <view class="fl fsize20 coE71415 lh25 mt5">
          <!-- <view class=" fl home-hot-tx rel  ml10" wx:if="{{item.continueFocus>=3}}">
            <view class="rel">{{item.continueFocus}}连红</view>
          </view>
          <view class=" fl home-hot-tx rel  ml10" wx:if="{{item.twenty_focus >=10&&item.articleCount>=20}}">
            <view class="rel">近20中{{item.twenty_focus}}</view>
          </view>
          <view class=" fl home-hot-tx rel  ml10" wx:if="{{(item.continueFocus<3||item.twenty_focus <10)&&item.articleCount>0}}">
            <view class="rel">总{{item.articleCount}}中{{item.focusArticleCount}}</view>
          </view> -->
          <view class=" fl home-hot-tx rel  ml10" wx:for="{{item.labelList}}" wx:for-item="it" wx:key="idx">
            <view class="rel">{{it}}</view>
          </view>

        </view>
        <view class="fr fsize20" wx:if="{{item.price==0}}">免费</view>
        <view class="fr coE71415 fsize20 pl30" wx:else
          style="background:url({{imgUrl+'home_ico3.png'}}) no-repeat center left/26rpx 26rpx;">{{item.price}}金币</view>

      </view>
    </navigator>
    <view class="list-none pb50" wx:if="{{list&&list.length==0}}">
      <image src="{{imgUrl+'no3.png'}}" class=" img3"></image>
      <view>暂无数据</view>
    </view>
  </view>

  <!-- 专家 -->
  <view class="pd24 pt25" wx:if="{{nav==3}}">
    <navigator hover-class="none" open-type="{{isHome?'reLaunch':'navigate'}}"
      url="/pages/expertArticle/expertArticle?id={{item.id}}&status=0" class="mb20 bgfff radius6 home-hot"
      wx:for="{{list}}" wx:key="index">
      <view class="f-cb mb10">
        <view class="rel fl mr20">
          <image src="{{item.avatar?item.avatar:imgUrl+'tx.png'}}" mode='aspectFill'
            class="block radius-50  wid86 hei86"></image>
          <image src="{{imgUrl+'home_ico4.png'}}" mode='aspectFill' class="abs bottom-0 right-0 wid24 hei24"></image>
        </view>
        <view class="fl fsize20 pt10">
          <view class="fsize28 mb5">{{item.nickName}}</view>
          <view class="coA0A0A0">{{item.createTime}}</view>
        </view>
        <view class="fr fsize20 coE71415 lh25 pt25">

          <view class=" fl home-hot-tx rel  ml10" wx:for="{{item.labelList}}" wx:for-item="it" wx:key="idx">
            <view class="rel">{{it}}</view>
          </view>
        </view>
      </view>

      <view class="fsize26 lh38 mb20 f-cb"><text class="cofff fsize18 bgE82F30 pl5 pr5 mr10 radius2 fl lh30 mt5"
          wx:if="{{item.notPointRefund==1}}">不中退金币</text>{{item.title}}</view>



      <view class=" pt20 pb20 f-cb radius6 coA0A0A0 center fsize22 mb20 rel" wx:for="{{item.match}}" wx:for-index="idx"
        wx:for-item="it" wx:key="idx"
        style=" background:#F2F2F2 url({{imgUrl+'home_ico7.png'}}) no-repeat center right 24rpx/13rpx 22rpx;">
        <view class="fl right wid130 box  pl10 pr10">{{it.match_name}}</view>
        <view class="fl {{it.home.length>8?'right':''}} wid188 co333 ">{{it.home}}</view>
        <view class="fl pl10 pr10" wx:if="{{it.status==2}}">{{it.home_scores}}-{{it.away_scores}}</view>
        <view class="fl pl10 pr10" wx:else>VS</view>

        <view class="fl {{it.away.length>8?'left':''}} wid188 co333 ">{{it.away}}</view>
        <image class="abs img2" src="{{imgUrl+'expert_ico8.png'}}" wx:if="{{item.focus==2}}"></image>
        <image class="abs img2" src="{{imgUrl+'expert_ico9.png'}}" wx:if="{{item.focus==1}}"></image>
      </view>

      <view class="fsize22 f-cb coA0A0A0 lh30">
        <view class="fl pr20 mr20 bor-riD8D8D8">足球</view>
        <view class="fl pr20 mr20 bor-riD8D8D8">开赛 {{item.mtime}}</view>
        <view class="fl pl30" style=" background:url({{imgUrl+'home_ico5.png'}}) no-repeat center left/22rpx 22rpx;">
          {{item.saleCount}}</view>

        <view class="fr fsize20" wx:if="{{item.price==0}}">免费</view>
        <view class="fr coE71415 fsize20 pl30" wx:else
